<resources>
	<properties>
    	<property id="currencyType" type="number">@Rez.Strings.currencyTypeCNY</property>
    </properties>
    <settings>
    	<setting propertyKey="@Properties.currencyType" title="@Strings.currencyType">
    		<settingConfig type="list">
                <listEntry value="0">@Strings.currencyTypeCNY</listEntry>
                <listEntry value="1">@Strings.currencyTypeUSD</listEntry>
                <listEntry value="2">@Strings.currencyTypeJPY</listEntry>
                <listEntry value="3">@Strings.currencyTypeEUR</listEntry>
    	    </settingConfig>
    	</setting>
    </settings>
</resources>