<resources>
	<properties>
    	<property id="currencyType" type="string">@Rez.Strings.currencyTypeCNY</property>
    </properties>
    <settings>
    	<setting propertyKey="@Properties.currencyType" title="@Strings.currencyType">
    		<settingConfig type="list">
                <listEntry value="cny">@Rez.Strings.currencyTypeCNY</listEntry>
                <listEntry value="usd">@Rez.Strings.currencyTypeUSD</listEntry>
                <listEntry value="jpy">@Rez.Strings.currencyTypeJPY</listEntry>
                <listEntry value="eur">@Rez.Strings.currencyTypeEUR</listEntry>
    	    </settingConfig>
    	</setting>
    </settings>
</resources>